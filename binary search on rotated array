#include <bits/stdc++.h>
using namespace std;

// Binary search on a Rotated Array.......


int SearchOn_Rotatearr(int arr[], int n, int key){
    int s=0;
    int e =n-1;
    while(s<=e){
        int mid= (s+e)/2;
        if(arr[mid] == key){
            return mid;
        } else if(arr[s]<= arr[mid]){
            if(key >= arr[s] && key <= arr[mid]){
                e = mid-1;
            } else {
                s = mid+1;
            }
        } else {
            if(key >= arr[mid] && key <= arr[e]){
                s = mid+1;
            } else{
                e = mid-1;
            }
        }
    }
    return -1;
}
int main() {
	int t;
	cin>>t;
	int arr[t];
	for(int i=0; i<t; i++){
	    cin>>arr[i];
	}
	int key;
	cin>>key;
	cout<<SearchOn_Rotatearr(arr, t , key)<<endl;
	return 0;
}
